{
  "manifest_version": 3,
  "key": "",
  "name": "Chrome Extension with React & Webpack",
  "description": "A chrome extension boilerplate built with React 17, Webpack 5, and Webpack Dev Server 4",
  "version": "0.1",
  "minimum_chrome_version": "33",
  "options_page": "options.html",
  "background": { "service_worker": "background.bundle.js" },
  "action": {
    "default_popup": "popup.html",
    "default_title": "WiseNET",
    "default_icon": "icon-34.png"
  },
  "chrome_url_overrides": {
    "newtab": "apps.html"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "<all_urls>"],
      "js": ["contentScript.bundle.js"],
      "css": ["content.styles.css"]
    }
  ],
  "devtools_page": "devtools.html",
  "web_accessible_resources": [
    {
      "resources": ["content.styles.css", "icon-128.png", "icon-34.png"],
      "matches": []
    }
  ],
  "permissions": [
    "tabs",
    "notifications",
    "bookmarks",
    "storage",
    "unlimitedStorage",
    "clipboardRead",
    "system.memory",
    "system.cpu",
    "*://localhost/*",
    "*://*/*",
    "https://cdnjs.cloudflare.com/*/",
    "https://www.google.com",
    "<all_urls>"
  ],
  "optional_permissions": ["unlimitedStorage"],
  "host_permissions": [
    "http://www.blogger.com/",
    "https://*.google.com/",
    "https://cdnjs.cloudflare.com/*/"
  ],
  "content_security_policy": {
    "default-src": "'self'",
    "connect-src": "* data: blob: filesystem:",
    "media-src": "* data: blob: filesystem:",
    "img-src": "'self'",
    "script-src": "'self'",
    "object-src": "'self'",
    "script-src-elem": "'self' 'wasm-unsafe-eval'",
    "style-src": "'self' fonts.googleapis.com;"
  }
}
