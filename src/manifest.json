{
  "manifest_version": 3,
  "name": "Chrome Extension with React & Webpack",
  "description": "A chrome extension boilerplate built with React 17, Webpack 5, and Webpack Dev Server 4",
  "options_page": "options.html",
  "background": { "service_worker": "background.bundle.js", "type": "module" },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "app": {
    "launch": {
      "local_path": "app.html"
    }
  },
  "chrome_url_overrides": {
    "newtab": "app.html"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "<all_urls>"],
      "js": ["contentScript.bundle.js"],
      "css": ["content.styles.css"]
    }
  ],
  "devtools_page": "devtools.html",
  "web_accessible_resources": [
    {
      "resources": [
        "content.styles.css",
        "icon-128.png",
        "icon-34.png",
        "devices.json",
        "assets/img/*",
        "assets/json/*"
      ],
      "matches": ["*://*/*"]
    }
  ],
  "host_permissions": [],
  "optional_host_permissions": ["*://*/*"],
  "permissions": ["nativeMessaging", "management", "storage", "file://*/"],
  "externally_connectable": {
    "ids": ["ihcdpceodailngfjicepeliafblopphg"]
  },
  "content_security_policy.extension_pages": "script-src 'self' https://example.com; object-src 'self'",
  "content_security_policy.sandbox": "script-src 'self' https://css-tricks.com; object-src 'self'"
}
