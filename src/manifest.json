{
  "manifest_version": 3,
  "name": "Chrome Extension with React & Webpack",
  "description": "A chrome extension boilerplate built with React 18, Webpack 6, and Webpack Dev Server 4",
  "Extension ID comment": [
    "aleblbammdopgjlaainhggebjfgfkpdf",
    "https://itero.plasmo.com/tools/generate-keypairs"
  ],
  "key": "",
  "options_page": "options.html",
  "background": { "service_worker": "background.bundle.js", "type": "module" },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "app": {
    "launch": {
      "local_path": "app.html"
    }
  },
  "chrome_url_overrides": {
    "newtab": "app.html"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "<all_urls>"],
      "js": ["contentScript.bundle.js"],
      "css": ["content.styles.css"]
    }
  ],
  "devtools_page": "devtools.html",
  "web_accessible_resources": [
    {
      "resources": [
        "content.styles.css",
        "icon-128.png",
        "icon-34.png",
        "devices.json",
        "assets/img/*",
        "assets/json/*"
      ],
      "matches": ["*://*/*"]
    }
  ],
  "host_permissions": [
    "<all_urls>",
    "http://*/"
  ],
  "optional_host_permissions": ["*://*/*"],
  "permissions": [
    "nativeMessaging",
    "management",
    "storage",
    "fileSystem",
    "file://*/"
  ],
  "externally_connectable": {
    "ids": ["ihcdpceodailngfjicepeliafblopphg"]
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';",
    "sandbox": "sandbox allow-scripts allow-forms allow-popups allow-modals; script-src 'self' 'unsafe-inline' 'unsafe-eval' 'wasm-unsafe-eval'; child-src 'self';"
  }  
}
